{
  "name": "express-api-boilerplate",
  "version": "1.0.0",
  "description": "Express.js API Boilerplate - Template lengkap untuk pengembangan REST API dengan fitur autentikasi, database, file upload, monitoring, dan lainnya",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "migrate": "knex --knexfile src/knexfile.js migrate:latest",
    "migrate:rollback": "knex --knexfile src/knexfile.js migrate:rollback",
    "migrate:make": "knex --knexfile src/knexfile.js migrate:make",
    "seed": "knex --knexfile src/knexfile.js seed:run",
    "seed:make": "knex --knexfile src/knexfile.js seed:make",
    "seed:all": "node src/repository/postgres/seeders/run_all_seeders.js",
    "consumer": "node src/scripts/start-consumer.js",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/express-api-boilerplate.git"
  },
  "author": "Your Name",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/your-username/express-api-boilerplate/issues"
  },
  "homepage": "https://github.com/your-username/express-api-boilerplate#readme",
  "dependencies": {
    "amqplib": "^0.10.7",
    "aws-sdk": "^2.1180.0",
    "axios": "^1.2.2",
    "bcrypt": "^6.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.6.1",
    "edge.js": "^5.3.7",
    "elastic-apm-node": "^4.7.0",
    "exceljs": "^4.3.0",
    "express": "^4.18.1",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^6.14.2",
    "i18n": "^0.15.0",
    "image-size": "^1.0.2",
    "jimp": "^1.6.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^3.1.2",
    "knex": "^2.1.0",
    "method-override": "^3.0.0",
    "minio": "^8.0.5",
    "moment": "^2.29.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.7.7",
    "pdfmake": "^0.2.6",
    "pg": "^8.7.3",
    "prom-client": "^15.1.3",
    "qrcode": "^1.5.4",
    "sharp": "^0.32.6",
    "swagger-ui-express": "^4.4.0",
    "uuid": "^12.0.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "eslint": "^8.19.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.1",
    "nodemon": "^3.1.10"
  }
}
