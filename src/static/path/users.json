	"/users/get": {
		"post": {
			"tags": ["Users"],
			"security": [
				{
					"bearerAuth": []
				}
			],
			"description": "Retrieve a paginated list of users with advanced filtering using POST method for complex queries",
			"requestBody": {
				"required": false,
				"content": {
					"application/json": {
						"schema": {
							"$ref": "#/components/schemas/SchemaPostUsersGet"
						}
					}
				}
			},
			"responses": {
				"200": {
					"description": "Users retrieved successfully",
