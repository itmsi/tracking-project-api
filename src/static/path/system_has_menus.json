{
	"/system-has-menus": {
		"get": {
			"tags": [
				"System Has Menus"
			],
			"summary": "Get all system-menu relationships with filtering, pagination, and sorting",
			"security": [
				{
					"bearerAuth": []
				}
			],
			"parameters": [
				{
					"name": "page",
					"in": "query",
					"required": false,
					"schema": {
						"type": "integer",
						"minimum": 1,
						"default": 1
					},
					"description": "Page number for pagination"
				},
				{
					"name": "limit",
					"in": "query",
					"required": false,
					"schema": {
						"type": "integer",
						"minimum": 1,
						"maximum": 100,
						"default": 10
					},
					"description": "Number of items per page (max 100)"
				},
				{
					"name": "sort_by",
					"in": "query",
					"required": false,
					"schema": {
						"type": "string",
						"enum": ["system_name", "menu_name", "created_at", "updated_at"],
						"default": "created_at"
					},
					"description": "Column to sort by"
				},
				{
					"name": "sort_order",
					"in": "query",
					"required": false,
					"schema": {
						"type": "string",
						"enum": ["asc", "desc"],
						"default": "desc"
					},
					"description": "Sort order (asc or desc)"
				},
				{
					"name": "search",
					"in": "query",
					"required": false,
					"schema": {
						"type": "string"
					},
					"description": "Search term for system_name or menu_name (case-insensitive)"
				},
				{
					"name": "system_id",
					"in": "query",
					"required": false,
					"schema": {
						"type": "string",
						"format": "uuid"
					},
					"description": "Filter by system ID"
				},
				{
					"name": "menu_id",
					"in": "query",
					"required": false,
					"schema": {
						"type": "string",
						"format": "uuid"
					},
					"description": "Filter by menu ID"
				},
				{
					"name": "start_date",
					"in": "query",
					"required": false,
					"schema": {
						"type": "string",
						"format": "date"
					},
					"description": "Filter by creation date (from)"
				},
				{
					"name": "end_date",
					"in": "query",
					"required": false,
					"schema": {
						"type": "string",
						"format": "date"
					},
					"description": "Filter by creation date (to)"
				}
			],
			"responses": {
				"200": {
					"description": "OK",
					"content": {
						"application/json": {
							"schema": {
								"$ref": "#/components/schemas/SchemaSystemHasMenusPaginatedResponse"
							}
						}
					}
				},
				"500": {
					"description": "Internal Server Error",
					"content": {
						"application/json": {
							"schema": {
								"$ref": "#/components/schemas/SchemaErrorResponse"
							}
						}
					}
				}
			}
		}
	},
	"/system-has-menus/{system_id}/{menu_id}": {
		"put": {
			"tags": [
				"System Has Menus"
			],
			"summary": "Update a system-menu relationship",
			"security": [
				{
					"bearerAuth": []
				}
			],
			"parameters": [
				{
					"in": "path",
					"name": "system_id",
					"required": true,
					"schema": {
						"type": "string",
						"format": "uuid"
					},
					"description": "System ID"
				},
				{
					"in": "path",
					"name": "menu_id",
					"required": true,
					"schema": {
						"type": "string",
						"format": "uuid"
					},
					"description": "Menu ID"
				}
			],
			"requestBody": {
				"required": true,
				"content": {
					"application/json": {
						"schema": {
							"$ref": "#/components/schemas/SchemaUpdateSystemHasMenu"
						}
					}
				}
			},
			"responses": {
				"200": {
					"description": "OK",
					"content": {
						"application/json": {
							"schema": {
								"$ref": "#/components/schemas/SchemaResponse"
							}
						}
					}
				},
				"404": {
					"description": "Not Found",
					"content": {
						"application/json": {
							"schema": {
								"$ref": "#/components/schemas/SchemaErrorResponse"
							}
						}
					}
				},
				"500": {
					"description": "Internal Server Error",
					"content": {
						"application/json": {
							"schema": {
								"$ref": "#/components/schemas/SchemaErrorResponse"
							}
						}
					}
				}
			}
		}
	},
	"/system-has-menus/system/{system_id}": {
		"get": {
			"tags": [
				"System Has Menus"
			],
			"summary": "Get all menus for a specific system",
			"security": [
				{
					"bearerAuth": []
				}
			],
			"parameters": [
				{
					"in": "path",
					"name": "system_id",
					"required": true,
					"schema": {
						"type": "string",
						"format": "uuid"
					},
					"description": "System ID"
				}
			],
			"responses": {
				"200": {
					"description": "OK",
					"content": {
						"application/json": {
							"schema": {
								"$ref": "#/components/schemas/SchemaResponse"
							}
						}
					}
				},
				"500": {
					"description": "Internal Server Error",
					"content": {
						"application/json": {
							"schema": {
								"$ref": "#/components/schemas/SchemaErrorResponse"
							}
						}
					}
				}
			}
		}
	}
}
