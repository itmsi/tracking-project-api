{
	"/import/master-data": {
		"post": {
			"tags": ["Import"],
			"summary": "Import master data from CSV",
			"description": "Import companies, departments, and titles from a CSV file",
			"security": [
				{
					"bearerAuth": []
				}
			],
			"requestBody": {
				"required": true,
				"content": {
					"multipart/form-data": {
						"schema": {
							"type": "object",
							"properties": {
								"csvFile": {
									"type": "string",
									"format": "binary",
									"description": "CSV file containing master data"
								}
							},
							"required": ["csvFile"]
						}
					}
				}
			},
			"responses": {
				"200": {
					"description": "Import completed successfully",
					"content": {
						"application/json": {
							"schema": {
								"$ref": "#/components/schemas/SchemaResponseImportResult"
							}
						}
					}
				},
				"400": {
					"description": "Bad request - validation error or duplicate data found",
					"content": {
						"application/json": {
							"schema": {
								"type": "object",
								"properties": {
									"message": {
										"type": "string"
									},
									"errors": {
										"type": "array",
										"items": {
											"$ref": "#/components/schemas/SchemaResponseImportDuplicate"
										}
									}
								}
							}
						}
					}
				},
				"401": {
					"description": "Unauthorized - Token required or invalid"
				},
				"500": {
					"description": "Internal server error"
				}
			}
		}
	},
	"/import/template": {
		"get": {
			"tags": ["Import"],
			"summary": "Get import template",
			"description": "Retrieve the CSV template structure for importing master data",
			"security": [
				{
					"bearerAuth": []
				}
			],
			"responses": {
				"200": {
					"description": "Import template retrieved successfully",
					"content": {
						"application/json": {
							"schema": {
								"$ref": "#/components/schemas/SchemaResponseImportTemplate"
							}
						}
					}
				},
				"401": {
					"description": "Unauthorized - Token required or invalid"
				},
				"500": {
					"description": "Internal server error"
				}
			}
		}
	}
}
